<h1> Edit Review!</h1>

<%= render partial: "layouts/errors", locals: {object: @reviews} %>

<%= form_for(@review) do |f|%>

    <%= f.label :rating %>
    <%= f.number_field :rating%><br>
            <br>
    <%= f.label :description %>
    <%= f.text_field :description%>

    <%= f.submit %>
<% end %>