<h1> Sign Up</h1>

<%=form_for(@user) do |f|%>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password%>
    
    <%= f.submit "Submit" %>

    
<% end %>